{
	"name": "charity-frontend",
	"author": {
		"name": "Sina Shafiee",
		"email": "alisinashafieezada@gmail.com"
	},
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Sina-shafiee/charity-frontend"
	},
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"prepare": "husky",
		"format": "prettier src/**/*.{ts,tsx} --write",
		"commitlint": "commitlint --edit",
		"commitizen": "exec < /dev/tty && git cz --hook || true",
		"codegen": "dotenv -e .env sh ./scripts/codegen"
	},
	"dependencies": {
		"@t3-oss/env-nextjs": "0.9.2",
		"next": "14.1.0",
		"next-intl": "3.2.1",
		"react": "18",
		"react-dom": "18",
		"server-only": "0.0.1",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "18.6.1",
		"@commitlint/config-conventional": "18.6.2",
		"@fullstacksjs/eslint-config": "10.10.1",
		"@types/node": "20",
		"@types/react": "18",
		"@types/react-dom": "18",
		"autoprefixer": "10.0.1",
		"commitizen": "4.3.0",
		"cz-conventional-changelog": "3.3.0",
		"dotenv-cli": "7.3.0",
		"eslint": "8",
		"eslint-config-next": "14.1.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.2",
		"openapi-typescript": "6.7.4",
		"postcss": "8",
		"prettier": "3.2.5",
		"tailwindcss": "3.4.0",
		"typescript": "5"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
