		"@svgr/webpack": "8.1.0",
		"@types/node": "20",
		"@types/react": "18",
		"@types/react-dom": "18",
		"autoprefixer": "10.0.1",
		"commitizen": "4.3.0",
		"cz-conventional-changelog": "3.3.0",
		"dotenv-cli": "7.3.0",
		"eslint": "8",
		"eslint-config-next": "14.1.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.2",
		"openapi-typescript": "6.7.4",
		"postcss": "8",
		"prettier": "3.2.5",
		"tailwindcss": "3.4.0",
		"typescript": "5"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
